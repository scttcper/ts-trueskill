{
  "name": "ts-trueskill",
  "version": "1.4.18",
  "description": "Port of python trueskill package in TypeScript",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "repository": "https://github.com/scttcper/ts-trueskill.git",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf build coverage docs benchmark-out .nyc_output",
    "build": "tsc -p . && cp package.json build && cp README.md build && cp LICENSE build",
    "release": "cd build && npm publish",
    "build:watch": "tsc -p . -w",
    "test": "mocha --require ts-node/register test/*.spec.ts",
    "coverage": "nyc npm run test",
    "docs": "typedoc --gitRevision master --out docs src; touch docs/.nojekyll",
    "lint": "tslint -p tsconfig.json --format codeFrame \"src/**/*.ts\"",
    "benchmark": "npm run build; tsc -p benchmark; node benchmark-out/benchmark"
  },
  "dependencies": {
    "gaussian": "^1.1.0",
    "lodash": "^4.17.5",
    "mathjs": "^4.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/chai": "4.1.2",
    "@types/gaussian": "1.1.1",
    "@types/lodash": "4.14.104",
    "@types/mathjs": "0.0.38",
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.6",
    "@types/uuid": "3.4.3",
    "benchmark": "2.1.4",
    "chai": "4.1.2",
    "mocha": "5.0.3",
    "nyc": "11.4.1",
    "prettier": "1.11.1",
    "rimraf": "2.6.2",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typedoc": "0.11.0",
    "typescript": "2.7.2"
  },
  "nyc": {
    "sourceMap": true,
    "instrument": true,
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": []
  },
  "keywords": [
    "typescript",
    "trueskill",
    "player skill"
  ]
}
