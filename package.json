{
  "name": "ts-trueskill",
  "version": "1.4.17",
  "description": "Port of python trueskill package in TypeScript",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "repository": "https://github.com/scttcper/ts-trueskill.git",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf build coverage docs benchmark-out .nyc_output",
    "build": "tsc -p . && cp package.json build && cp README.md build && cp LICENSE build",
    "release": "cd build && npm publish",
    "build:watch": "tsc -p . -w",
    "test": "mocha --require test/tshook.js test/*.spec.ts",
    "coverage": "nyc npm run test",
    "docs": "typedoc --gitRevision master --out docs src; touch docs/.nojekyll",
    "lint": "tslint -p tsconfig.json --format codeFrame \"src/**/*.ts\"",
    "benchmark": "npm run build; tsc -p benchmark; node benchmark-out/benchmark"
  },
  "dependencies": {
    "gaussian": "^1.1.0",
    "lodash": "^4.17.4",
    "mathjs": "^3.18.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/chai": "^4.0.10",
    "@types/gaussian": "^1.1.1",
    "@types/lodash": "^4.14.91",
    "@types/mathjs": "^0.0.36",
    "@types/mocha": "^2.2.45",
    "@types/node": "^8.5.5",
    "@types/uuid": "^3.4.3",
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2"
  },
  "nyc": {
    "sourceMap": true,
    "instrument": true,
    "require": [
      "./test/tshook.js"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": []
  },
  "keywords": [
    "typescript",
    "trueskill",
    "player skill"
  ]
}
