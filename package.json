{
  "name": "ts-trueskill",
  "version": "0.0.0",
  "description": "Port of python trueskill package in TypeScript",
  "main": "./public_api.js",
  "module": "./bundles/ts-trueskill.es2015.js",
  "typings": "./public_api.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/scttcper/ts-trueskill.git",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "benchmark": "npm run build; tsc -p benchmark; node benchmark-out/benchmark",
    "lint": "tslint -p tsconfig.json -t stylish 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "lint-staged": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json && ts-node ./build.ts",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run test -- --coverage --no-cache",
    "commit": "git-cz",
    "commitlint": "commitlint -e $GIT_PARAMS",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "cd dist/package-dist && semantic-release"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "mathjs": "^4.1.2",
    "ts-gaussian": "^1.1.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "6.1.3",
    "@commitlint/config-conventional": "6.1.3",
    "@types/benchmark": "1.0.31",
    "@types/fs-extra": "5.0.2",
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.108",
    "@types/mathjs": "3.20.0",
    "@types/node": "9.6.8",
    "@types/uuid": "3.4.3",
    "benchmark": "2.1.4",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "fs-extra": "5.0.0",
    "husky": "0.15.0-rc.13",
    "jest": "22.4.3",
    "lint-staged": "7.0.5",
    "prettier": "1.12.1",
    "rimraf": "2.6.2",
    "rollup": "0.58.2",
    "rollup-plugin-sourcemaps": "0.4.2",
    "semantic-release": "15.1.7",
    "travis-deploy-once": "5.0.0",
    "ts-jest": "22.4.4",
    "ts-node": "6.0.1",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.12.0",
    "tslint-config-standard": "7.0.0",
    "typedoc": "0.11.1",
    "typescript": "2.8.3"
  },
  "release": {
    "branch": "master"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "npm run commitlint"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "collectCoverage": true
  },
  "keywords": [
    "typescript",
    "trueskill",
    "player skill"
  ]
}
