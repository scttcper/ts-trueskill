{
  "name": "ts-trueskill",
  "version": "0.0.0-placeholder",
  "description": "Port of python trueskill package in TypeScript",
  "repository": "scttcper/ts-trueskill",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/module/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "benchmark": "npx ts-node -P ./benchmark/tsconfig.json benchmark/index.ts",
    "lint": "eslint --ext .js,.ts, . --format codeframe",
    "lint:fix": "eslint --fix --ext .js,.ts, . --format codeframe",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "build:docs": "typedoc --out docs --hideGenerator --target ES6 --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache"
  },
  "dependencies": {
    "mathjs": "^7.2.0",
    "ts-gaussian": "^2.0.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@ctrl/eslint-config": "1.1.0",
    "@jest/globals": "26.4.2",
    "@types/benchmark": "1.0.33",
    "@types/lodash": "4.14.164",
    "@types/mathjs": "6.0.5",
    "@types/node": "14.11.2",
    "@types/uuid": "8.3.0",
    "benchmark": "2.1.4",
    "jest": "26.4.2",
    "lodash": "4.17.20",
    "typedoc": "0.19.2",
    "typescript": "4.0.5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "typescript",
    "trueskill",
    "ranking",
    "player skill"
  ]
}
