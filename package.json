{
  "name": "ts-trueskill",
  "version": "0.0.0-placeholder",
  "description": "Port of python trueskill package in TypeScript",
  "source": "src/public_api.ts",
  "main": "./ts-trueskill.js",
  "module": "./ts-trueskill.m.js",
  "typings": "./src/public_api.d.ts",
  "sideEffects": false,
  "repository": "scttcper/ts-trueskill",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "benchmark": "ts-node -P ./benchmark/tsconfig.json benchmark/index.ts",
    "lint": "tslint -p tsconfig.json -t stylish",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix",
    "prebuild": "rimraf dist",
    "build": "microbundle build --target node --format cjs,es -o dist",
    "postbuild": "rimraf dist/test && rimraf dist/benchmark && cp package.json dist && cp LICENSE dist && cp README.md dist",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run test -- --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "lodash.flatten": "^4.4.0",
    "lodash.merge": "^4.6.1",
    "lodash.zipobject": "^4.1.3",
    "mathjs": "^5.4.0",
    "ts-gaussian": "^1.1.11",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/fs-extra": "5.0.4",
    "@types/jest": "23.3.13",
    "@types/lodash.flatten": "4.4.4",
    "@types/lodash.merge": "4.6.4",
    "@types/lodash.zipobject": "4.1.4",
    "@types/mathjs": "4.4.5",
    "@types/node": "10.12.19",
    "@types/uuid": "3.4.4",
    "benchmark": "2.1.4",
    "jest": "23.6.0",
    "microbundle": "0.9.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "travis-deploy-once": "5.0.11",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-config-standard": "8.0.1",
    "tslint-xo": "0.11.0",
    "typedoc": "0.14.0",
    "typescript": "3.2.2"
  },
  "release": {
    "branch": "master"
  },
  "keywords": [
    "typescript",
    "trueskill",
    "player skill"
  ]
}
