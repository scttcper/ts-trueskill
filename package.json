{
  "name": "ts-trueskill",
  "version": "0.0.0-placeholder",
  "description": "Port of python trueskill package in TypeScript",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "module": "./module/index.js",
  "sideEffects": false,
  "repository": "scttcper/ts-trueskill",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "benchmark": "ts-node -P ./benchmark/tsconfig.json benchmark/index.ts",
    "lint": "eslint src/**/*.ts test/**/*.ts benchmark/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.module.json",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run test -- --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "@types/mathjs": "5.0.0",
    "lodash": "^4.17.11",
    "mathjs": "^5.5.0",
    "ts-gaussian": "^1.1.11",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/fs-extra": "5.0.5",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/node": "11.9.6",
    "@types/uuid": "3.4.4",
    "@typescript-eslint/eslint-plugin": "1.4.1",
    "@typescript-eslint/parser": "1.4.1",
    "benchmark": "2.1.4",
    "eslint": "5.14.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.8.0",
    "eslint-plugin-import": "2.16.0",
    "jest": "24.1.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "ts-jest": "24.0.0",
    "ts-node": "8.0.2",
    "typedoc": "0.14.2",
    "typescript": "3.3.3333"
  },
  "release": {
    "branch": "master"
  },
  "keywords": [
    "typescript",
    "trueskill",
    "ranking",
    "player skill"
  ]
}
