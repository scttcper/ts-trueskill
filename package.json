{
  "name": "ts-trueskill",
  "version": "1.2.0",
  "description": "Port of python trueskill package in TypeScript",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "repository": "https://github.com/scttcper/ts-trueskill.git",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "build:watch": "tsc -p . -w",
    "test": "mocha --require ts-node/register test/*.spec.ts",
    "coverage": "nyc npm test",
    "docs": "typedoc --gitRevision master --out docs --exclude \"**/*+(spec).ts\"; touch docs/.nojekyll",
    "lint": "tslint \"src/**/*.ts\""
  },
  "dependencies": {
    "gaussian": "^1.1.0",
    "lodash": "^4.17.4",
    "mathjs": "^3.13.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/gaussian": "^1.1.1",
    "@types/lodash": "^4.14.64",
    "@types/mathjs": "^0.0.34",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "@types/uuid": "^2.0.29",
    "chai": "^3.5.0",
    "mocha": "^3.4.1",
    "nyc": "^10.3.2",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typedoc": "^0.7.0",
    "typescript": "^2.3.2"
  },
  "nyc": {
    "sourceMap": true,
    "instrument": true,
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": []
  },
  "keywords": [
    "typescript",
    "trueskill",
    "player skill"
  ]
}
