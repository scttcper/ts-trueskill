{
  "name": "ts-trueskill",
  "version": "0.0.0-placeholder",
  "description": "Port of python trueskill package in TypeScript",
  "repository": "scttcper/ts-trueskill",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "main": "./dist/index.js",
  "module": "./dist/module/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "benchmark": "ts-node -P ./benchmark/tsconfig.json benchmark/index.ts",
    "lint": "eslint src/**/*.ts test/**/*.ts benchmark/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "build": "del dist && tsc && tsc -p tsconfig.module.json",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@types/mathjs": "5.0.1",
    "lodash": "^4.17.11",
    "mathjs": "^5.10.3",
    "ts-gaussian": "^1.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/fs-extra": "7.0.0",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.135",
    "@types/node": "12.0.10",
    "@types/uuid": "3.4.4",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "del-cli": "1.1.0",
    "benchmark": "2.1.4",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.12.0",
    "eslint-plugin-import": "2.17.3",
    "jest": "24.8.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.18",
    "ts-jest": "24.0.2",
    "ts-node": "8.2.0",
    "typedoc": "0.15.0-0",
    "typescript": "3.4.5"
  },
  "release": {
    "branch": "master"
  },
  "keywords": [
    "typescript",
    "trueskill",
    "ranking",
    "player skill"
  ]
}
