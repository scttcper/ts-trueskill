{
  "name": "ts-trueskill",
  "version": "0.0.0",
  "description": "Port of python trueskill package in TypeScript",
  "main": "./public_api.js",
  "module": "./es/public_api.js",
  "typings": "./public_api.d.ts",
  "sideEffects": false,
  "repository": "scttcper/ts-trueskill",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "benchmark": "npm run build; tsc -p benchmark; node benchmark-out/benchmark",
    "lint": "tslint -p tsconfig.json -t stylish 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "npm run build-es6; npm run build-cmjs; ts-node build",
    "build-es6": "tsc -P ./tsconfig-build.json --outDir 'dist/es' --module es2015 -d false --moduleResolution node",
    "build-cmjs": "tsc -P ./tsconfig-build.json",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run test -- --coverage --no-cache",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "cd dist/package-dist && semantic-release"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "mathjs": "^5.0.0",
    "ts-gaussian": "^1.1.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/fs-extra": "5.0.3",
    "@types/jest": "23.1.3",
    "@types/lodash": "4.14.110",
    "@types/mathjs": "4.4.0",
    "@types/node": "10.3.5",
    "@types/uuid": "3.4.3",
    "benchmark": "2.1.4",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "fs-extra": "6.0.1",
    "jest": "23.1.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.6.0",
    "travis-deploy-once": "5.0.0",
    "ts-jest": "22.4.6",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-config-standard": "7.1.0",
    "typedoc": "0.11.1",
    "typescript": "2.9.2"
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "collectCoverage": true
  },
  "keywords": [
    "typescript",
    "trueskill",
    "player skill"
  ]
}
